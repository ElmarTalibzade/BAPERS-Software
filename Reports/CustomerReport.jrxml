	<queryString>
		<![CDATA[select tasks.jobCode, tasks.taskID, tasks.price, tasks.description, tasks.startTime, timestampdiff(MINUTE, tasks.startTime, tasks.endTime) as timeTaken,
(CASE WHEN tasks.status = 0 THEN 'Unspecified'
	WHEN tasks.status = 1 THEN 'Inactive'
	WHEN tasks.status = 2 THEN 'In Progress' 
	WHEN tasks.status = 3 THEN 'Completed' END) as status,
(SELECT enum_departments.name FROM enum_departments WHERE enum_departments.index = tasks.department) as department,
customers.accountNo, holderName, firstName, lastName, address, phoneNo, emailAddress
from tasks 
inner join jobs on 
tasks.jobCode = jobs.code 
inner join customers on 
customers.accountNo = jobs.ownerNo
where tasks.startTime BETWEEN $P{DATE_START} AND $P{DATE_END} AND customers.accountNo = $P{CUSTOMER_NO}]]>
	</queryString>
